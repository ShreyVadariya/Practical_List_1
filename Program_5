// Write C++ program to swap two numbers.

#include <iostream>
using namespace std;

int main()
{
    int a = 5, b = 10, temp;

    cout<<"My Name Is Vadariya Shrey"<<endl;
    cout<<"En_no :: 220130318054 "<<endl;
    cout << "Before swapping." << endl;
    cout << "a = " << a << ", b = " << b << endl;

    temp = a;
    a = b;
    b = temp;

    cout << "\nAfter swapping." << endl;
    cout << "a = " << a << ", b = " << b << endl;

    return 0;
}
